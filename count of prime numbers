def isprime(n):
    for i in range(2,n):
        if n%i==0:
            return False
    return True

t=int(input())        
for i in range(t):
    n=input().split()
    x=int(n[0])
    y=int(n[1])
    c=0
    for i in range(x+1,y):
        if isprime(i):
            c=c+1
    print(c)
    
    
  Output:
5
1 10
4
2 10
3
4 6
1
3 5
0
100 102
1
